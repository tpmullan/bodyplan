<%= simple_form_for(@review) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id, as: 'hidden' %>
    <%= f.input :program_id, as: 'hidden' %>
    <%= f.input :body %>
    <%= f.input :rating %>
    <%= f.input :title %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
