<div class="container-fluid background-color" style="padding-left: 0;">
  <div class="col-xs-12 col-sm-4 col-md-3 col-lg-2 filter-menu">
    <h4>Sort by: </h4>
    <div class="input-group">
      <span class="input-group-btn">
	    <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Popularity <span class="caret"></span></button>
	    <ul class="dropdown-menu">
	      <li><a href="#">Recent</a></li>
	      <li><a href="#">Price: Low to High</a></li>
	      <li><a href="#">Price: High to Low</a></li>
	      <li><a href="#">Rating</a></li>
	    </ul>
      </span>
    </div>
    <div class="side-divider"></div>
      <h4>Price: </h4>
 	  <ul class="sidemenu-list">
 	  	<li>Free <span class="pull-right">(20)</span></li>
 	  	<li>Less than $29<span class="pull-right">(59)</span></li>
 	  	<li>$30 - $59<span class="pull-right">(235)</span></li>
 	  	<li>$60 - $99<span class="pull-right">(24)</span></li>
 	  	<li>$100 - $199<span class="pull-right">(40)</span></li>
 	  	<li>$200+<span class="pull-right">(14)</span></li>
 	  </ul>
    <div class="side-divider"></div>
      <h4>Difficulty: </h4>
      <ul class="sidemenu-list">
 	  	<li>All <span class="pull-right">(999+)</span></li>
 	  	<li>Beginner<span class="pull-right">(503)</span></li>
 	  	<li>Intermediate<span class="pull-right">(999+)</span></li>
 	  	<li>Advanced<span class="pull-right">(23)</span></li>
 	  </ul>
    <div class="side-divider"></div>
  </div>
  <div class="program-overviewcontent col-xs-12 col-sm-8 col-md-9 col-lg-10 col-sm-offset-4 col-md-offset-3 col-lg-offset-2">
    <div class=" program-index">
  	  <h1>Fitness Programs</h1>
  	  <form class="navbar-form" role="search">
		  <div class="input-group">
			<input type="text" class="form-control" id="search-programs" placeholder="Search Programs" name="srch-term" id="srch-term">
			<div class="input-group-btn">
				<button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
			</div>
		  </div>
	  </form>
  	  <h4>Search for '<span>something</span>' </h4>
    </div>
    <div class="search-results programs">
      <div class="container individual-programs margin-zero">
	    <%= render :partial => "result", :collection => @results %>
      </div>
      <%= will_paginate @results %>
    </div>
  </div>
</div>