<div class="col-xs-3">
  <div class="program-holder subscriptions">
    <%= link_to subscriptions do %>
    <div class="program-header", style="background-image: url(<%= image_path(subscriptions.cover_photo.url) %>);"></div>
    <div class="program-content">
      <h4 class="text-center"><%= subscriptions.name %></h4>
      <div class="progress">
        <div class="progress-bar  progress-referral submit" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 78%;">
        </div>
      </div>
      <p>75% Complete</p>
    </div>
    <% end %>
  </div>
</div>
